{
  "parser": "babel-eslint",
  "settings": {
    "ecmascript": 6,
    "jsx": true,
    "node": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "strict": 1,
    "global-strict": true,
    "semi": false,
    "no-use-before-define": false,
    "quotes": [2, "single"],
    "no-unused-vars": false,
    "no-underscore-dangle": false
  },
}
